{
  "buildOptions": {
    "additionalArguments": [
      "/ruleset:../../build/Analyzers.ruleset"
    ],
    "embed": {
      "include": [
        "**/*.resx",
        "Files/*.*"
      ]
    },
    "keyFile": "../../Autofac.snk"
  },
  "dependencies": {
    "Autofac.Configuration": {
      "target": "project"
    },
    "Microsoft.Extensions.Configuration": "1.0.0",
    "Microsoft.Extensions.Configuration.Json": "1.0.0",
    "Microsoft.Extensions.Configuration.Xml": "1.0.0",
    "Moq": "4.6.25-alpha",
    "Microsoft.NETCore.Platforms": "1.0.1",
    "StyleCop.Analyzers": {
      "type": "build",
      "version": "1.0.0"
    },
    "dotnet-test-xunit": "2.2.0-*",
    "xunit": "2.2.0-*"
  },
  "frameworks": {
    "net46": {}
  },
  "packOptions": {
    "authors": [
      "Autofac Contributors"
    ]
  },
  "testRunner": "xunit"
}