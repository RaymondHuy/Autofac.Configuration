{
    "version": "4.0.0-*",
    "authors": ["Autofac Contributors"],
    "description": "Configuration support for Autofac",
    "summary": "Enables Autofac dependencies to be set up using configuration sources.",
    "projectUrl": "http://autofac.org",
    "licenseUrl": "http://www.opensource.org/licenses/mit-license.php",
    "iconUrl": "http://code.google.com/p/autofac/logo",
    "requireLicenseAcceptance": false,
    "tags": [ "Autofac", "DI", "IoC" ],
    "compilationOptions": {
        "warningsAsErrors": true
    },
    "compile": ["**\\*.cs"],
    "namedResource": {
        "Autofac.Configuration.ConfigurationResources": "Properties/ConfigurationResources.resx"
    },
    "dependencies": {
        "Autofac": "4.0.0-*",
        "Microsoft.Framework.ConfigurationModel": "1.0.0-*"
    },
    "frameworks": {
        "net45": {
            "frameworkAssemblies": {
                "System.Collections": "",
                "System.ComponentModel": "",
                "System.Globalization": "",
                "System.Linq": "",
                "System.Reflection": "",
                "System.Reflection.Extensions": "",
                "System.Resources.ResourceManager": ""
            }
        },
        "dnx451": {
            "frameworkAssemblies": {
                "System.Collections": "",
                "System.ComponentModel": "",
                "System.Globalization": "",
                "System.IO": "",
                "System.Linq": "",
                "System.Reflection": "",
                "System.Reflection.Extensions": "",
                "System.Resources.ResourceManager": ""
            }
        },
        "dnxcore50": {
            "dependencies": {
                "System.Collections": "4.0.10-beta-*",
                "System.ComponentModel.TypeConverter": "4.0.0-beta-*",
                "System.Diagnostics.Tools": "4.0.0-beta-*",
                "System.Globalization": "4.0.10-beta-*",
                "System.IO": "4.0.10-beta-*",
                "System.Linq": "4.0.0-beta-*",
                "System.Reflection": "4.0.10-beta-*",
                "System.Reflection.Extensions": "4.0.0-beta-*",
                "System.Reflection.TypeExtensions": "4.0.0-beta-*",
                "System.Resources.ResourceManager": "4.0.0-beta-*"
            }
        }
    }
}