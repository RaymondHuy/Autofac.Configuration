{
    "version": "4.0.0-beta7-*",
    "authors": ["Autofac Contributors"],
    "description": "Configuration support for Autofac",
    "summary": "Enables Autofac dependencies to be set up using configuration sources.",
    "projectUrl": "http://autofac.org",
    "licenseUrl": "http://www.opensource.org/licenses/mit-license.php",
    "iconUrl": "http://code.google.com/p/autofac/logo",
    "requireLicenseAcceptance": false,
    "tags": [ "Autofac", "DI", "IoC" ],
    "compilationOptions": {
        "warningsAsErrors": true
    },
    "compile": ["**/*.cs"],
    "resource": ["**/*.resx"],
    "repository": {
        "type": "git",
        "url": "https://github.com/autofac/Autofac"
    },
    "dependencies": {
        "Autofac": "4.0.0-beta7-*",
        "Microsoft.Framework.Configuration": "1.0.0-beta7"
    },
    "frameworks": {
        "dnx451": {},
        "dotnet": {
            "dependencies": {
                "System.Collections": "4.0.11-beta-23225",
                "System.ComponentModel.TypeConverter": "4.0.1-beta-23225",
                "System.Diagnostics.Tools": "4.0.1-beta-23225",
                "System.Globalization": "4.0.11-beta-23225",
                "System.IO": "4.0.11-beta-23225",
                "System.Linq": "4.0.1-beta-23225",
                "System.Reflection": "4.1.0-beta-23225",
                "System.Reflection.Extensions": "4.0.1-beta-23225",
                "System.Reflection.TypeExtensions": "4.0.1-beta-23225",
                "System.Resources.ResourceManager": "4.0.1-beta-23225"
            }
        },
        "net45":{
            "frameworkAssemblies": {
                "System.Collections": "",
                "System.ComponentModel": "",
                "System.Globalization": "",
                "System.Linq": "",
                "System.Reflection": "",
                "System.Reflection.Extensions": "",
                "System.Resources.ResourceManager": ""
            }
        }
    }
}