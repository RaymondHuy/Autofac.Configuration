{
    "version": "4.0.0-rc2-*",
    "authors": ["Autofac Contributors"],
    "description": "Configuration support for Autofac",
    "summary": "Enables Autofac dependencies to be set up using configuration sources.",
    "projectUrl": "http://autofac.org",
    "licenseUrl": "http://www.opensource.org/licenses/mit-license.php",
    "iconUrl": "http://code.google.com/p/autofac/logo",
    "requireLicenseAcceptance": false,
    "tags": [ "Autofac", "DI", "IoC" ],
    "compilationOptions": {
        "warningsAsErrors": true
    },
    "compile": ["**/*.cs"],
    "resource": ["**/*.resx"],
    "repository": {
        "type": "git",
        "url": "https://github.com/autofac/Autofac.Configuration"
    },
    "dependencies": {
        "Autofac": "4.0.0-rc2-*",
        "Microsoft.Extensions.Configuration": "1.0.0-rc2-*"
    },
    "frameworks": {
        "dotnet5.4": {
            "dependencies": {
                "System.Collections": "4.0.11-beta-*",
                "System.ComponentModel.TypeConverter": "4.0.1-beta-*",
                "System.Diagnostics.Tools": "4.0.1-beta-*",
                "System.Globalization": "4.0.11-beta-*",
                "System.IO": "4.0.11-beta-*",
                "System.Linq": "4.0.1-beta-*",
                "System.Reflection": "4.1.0-beta-*",
                "System.Reflection.Extensions": "4.0.1-beta-*",
                "System.Reflection.TypeExtensions": "4.0.1-beta-*",
                "System.Resources.ResourceManager": "4.0.1-beta-*"
            }
        },
        "net451": {},
        "netcore50": {
            "dependencies": {
                "System.Collections": "4.0.11-beta-*",
                "System.ComponentModel.TypeConverter": "4.0.1-beta-*",
                "System.Diagnostics.Tools": "4.0.1-beta-*",
                "System.Globalization": "4.0.11-beta-*",
                "System.IO": "4.0.11-beta-*",
                "System.Linq": "4.0.1-beta-*",
                "System.Reflection": "4.1.0-beta-*",
                "System.Reflection.Extensions": "4.0.1-beta-*",
                "System.Reflection.TypeExtensions": "4.0.1-beta-*",
                "System.Resources.ResourceManager": "4.0.1-beta-*"
            }
        }
    }
}